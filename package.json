{
  "name": "notebook",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:ios": "cap run ios --target-name 'iPhone 17'",
    "build": "vite build",
    "build:ios": "bun run build && cap sync ios && cap build ios",
    "upload:ios": "dotenv -e .env -- sh -c 'xcrun altool --upload-app --type ios --file \"ios/App/output/App.ipa\" --apiKey $APPLE_API_KEY_ID --apiIssuer $APPLE_API_ISSUER'",
    "preview": "vite preview",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@capacitor-community/sqlite": "^8.0.0",
