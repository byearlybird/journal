{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:*"
        },
        {
          "url": "https://*.byearlybird.com/*"
        }
      ]
    },
    "sql:default",
    "sql:allow-execute"
  ]
}
